<Window x:Class="OkulDB.Sınıf"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OkulDB"
        mc:Ignorable="d"
        Title="Sınıf" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtSınıfad" HorizontalAlignment="Left" Height="23" Margin="284,51,0,0" TextWrapping="Wrap" Text="Sınıf Adı" VerticalAlignment="Top" Width="153"/>
        <TextBox x:Name="txtkontenjan" HorizontalAlignment="Left" Height="23" Margin="284,88,0,0" TextWrapping="Wrap" Text="Kontenjan" VerticalAlignment="Top" Width="153"/>
        <Label Content="Sınıf Adı" HorizontalAlignment="Left" Margin="216,51,0,0" VerticalAlignment="Top" Height="23" Width="66"/>
        <Label Content="Kontenjan" HorizontalAlignment="Left" Margin="216,88,0,0" VerticalAlignment="Top" Width="63"/>
        <Button x:Name="btnsnfkaydet" Content="Kaydet" HorizontalAlignment="Left" Margin="314,125,0,0" VerticalAlignment="Top" Width="65" Height="19" Click="btnsnfkaydet_Click"/>

    </Grid>
</Window>
